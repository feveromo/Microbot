**/*.class
**/*.jar
**/*.war
**/*.ear

# IDE settings and workspace files
.idea/
.vscode/
.run/
.mvn/
.gradle/

# Version control and CI metadata
.git/
.github/

# Cursor internal workspace data
# .cursor/

# Dependency directories
node_modules/

# Cache modules generated by RuneLite or other tools
cache/

# Media and other large binary assets (optional)
*.png
*.jpg
*.jpeg
*.gif
*.webp
*.bmp
*.ico
*.svg
*.pdf

# Logs
*.log

# Test files
runelite-client/src/test